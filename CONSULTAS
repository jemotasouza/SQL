---Executando consultas para verificar se os dados foram importados corretamente (Select)
SELECT * FROM ALUNOS;
SELECT * FROM PROFESSORES;
SELECT * FROM NOTAS;
SELECT * FROM TURMAS;

---Retorne as informações de todos os alunos ordenados pelo nome (Order By)
SELECT * FROM ALUNOS
ORDER BY NOME_ALUNO ASC

---Retornar a disciplina que possui a carga horaria maior que 40 (Comparação)
SELECT * FROM DISCIPLINA
WHERE CARGA_HORARIA > 40;

---Buscar as notas que são maiores que 6 e menores que 8 (
SELECT * FROM NOTAS
WHERE VALOR_NOTA > 6 AND VALOR_NOTA < 8;

---Listar todos os alunos do gênero feminino
SELECT * FROM ALUNOS
WHERE GENERO = 'F';

---Mostrar os alunos que nasceram antes do ano 2005
SELECT * FROM ALUNOS
WHERE DATA_NASCIMENTO < '01-01-2005';
